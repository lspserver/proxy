{
  "author": "craftslab",
  "name": "proxy",
  "version": "1.0.0",
  "description": "LSP proxy",
  "productName": "lspserver",
  "license": "Apache-2.0",
  "bin": "dist/proxy.js",
  "main": "dist/proxy.js",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "coverage": "mkdir coverage && nyc report --reporter=json -t coverage --report-dir=coverage",
    "dev": "tsc -w",
    "lint": "eslint --ext .ts ./",
    "start": "yarn run clean && yarn run build && node dist/proxy.js --port 49093 --servers config.yml",
    "test": "nyc mocha"
  },
  "dependencies": {
    "@sourcegraph/vscode-ws-jsonrpc": "^0.0.3-fork",
    "copy-webpack-plugin": "^5.1.1",
    "js-yaml": "^3.13.1",
    "minimist": "^1.2.6",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/node": "^10.14.8",
    "@types/ws": "^6.0.1",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "rimraf": "^2.6.3",
    "ts-loader": "^7.0.0",
    "typescript": "^3.9.10",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
